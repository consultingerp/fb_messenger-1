    <template id="ks_rating_widget_stars_static" name="Rating: static star widget">
        <t t-set="rating_avg" t-value="round(rating_avg * 100) / 100 / 2"/>
        <t t-set="val_decimal" t-value="round(rating_avg % 1, 1)"/>
        <t t-set="val_integer" t-value="int(rating_avg)"/>
        <t t-set="empty_star" t-value="5 - (val_integer+1) if val_decimal else 5 - val_integer"/>
        <div class="o_website_rating_static" t-att-style="inline_mode and 'display:inline'">
            <t t-foreach="range(0, val_integer)" t-as="num">
                <i class="fa fa-star" role="img" aria-label="One star" title="One star"></i>
            </t>
            <t t-if="val_decimal">
                <i class="fa fa-star-half-o" role="img" aria-label="Half a star" title="Half a star"></i>
            </t>
            <t t-foreach="range(0, empty_star)" t-as="num">
                <i class="fa fa-star text-black-25" role="img"></i>
            </t>
            (<t t-esc="rating_count"/>)
        </div>
    </template>
<!--    it is replace exiting suggestive option-->
    <template id='website_sale_comparison.recommended_product' inherit_id="website_sale.recommended_products" name="Alternative Products"/>
     <template id='suggested products' inherit_id="ks_theme_kinetik.kps_product" name="Alternative Products">
            <xpath expr="//a[@id='add_to_cart']" position="after">
            <!--
                Create the first variant if it doesn't exist for the current
                product and every alternative product because the product
                comparator need the variants and not the templates.
            -->
            <t t-set="product_variant" t-value="product_variant or product._create_first_product_variant()"/>
            <t t-set="id_list" t-value="[product_variant.id] if product_variant else []"/>
            <t t-foreach="product.alternative_product_ids" t-as="alt_product">
                <t t-set="alt_product_variant_id" t-value="alt_product._create_first_product_variant().id"/>
                <t t-if="alt_product_variant_id" t-set="id_list" t-value="id_list + [alt_product_variant_id]"/>
            </t>
            <t t-set="ids" t-value="','.join([str(id) for id in id_list])"/>
            <div class="ks_suggested_products">
                <a t-if="len(id_list) > 1" role="button" class="btn btn-compare" data-toggle="tooltip" data-placement="top" title="Compare" t-attf-href="/shop/compare/?products=#{ids}">
                    <i class="fa fa-exchange"></i>
                </a>
            </div>
        </xpath>
    </template>
     <template id="website_sale.product_price">
      <div itemprop="offers" itemscope="itemscope" itemtype="http://schema.org/Offer" class="product_price mt16">
          <h4 class="oe_price_h4 css_editable_mode_hidden">
              <b class="oe_price" style="white-space: nowrap;" t-esc="combination_info['price']" t-options="{'widget': 'monetary', 'display_currency': website.currency_id}"/>
              <span itemprop="price" style="display:none;" t-esc="combination_info['price']"/>
              <span itemprop="priceCurrency" style="display:none;" t-esc="website.currency_id.name"/>
               <span t-attf-class="ks_public_price oe_default_price {{'' if combination_info['has_discounted_price'] else 'd-none'}}" style="text-decoration: line-through; white-space: nowrap;"
                  t-esc="combination_info['list_price']" t-options="{'widget': 'monetary', 'display_currency': website.currency_id}"
              />
              <t t-if="request.website.viewref('ks_theme_kinetik.percent_discount').active">
                  <div class="ks_snippet_percentage_discount_2">
                      <div class="Percentage-offer">

                      </div>
                  </div>
              </t>
          </h4>
          <h4 class="css_non_editable_mode_hidden decimal_precision" t-att-data-precision="str(website.currency_id.decimal_places)">
            <span t-field="product.list_price"
                t-options='{
                   "widget": "monetary",
                   "display_currency": product.currency_id,
               }'/>
          </h4>
          <h4 class="d-none oe_not_available bg-warning">Option not available</h4>
      </div>
    </template>
    <template id="website_sale_stock.website_sale_stock_product" inherit_id="ks_theme_kinetik.kps_product" priority="4">
        <xpath expr="//p[@id='ks_alert_wrng']" position="before">
            <div class="availability_messages o_not_editable"/>
        </xpath>
    </template>

    <template id="website_sale.shop_product_carousel" name="Shop Product Carousel">
        <t t-if="request.website.viewref('ks_theme_kinetik.ks_multiimage_layout').active">
            <t t-call="ks_theme_kinetik.ks_product_image_slider">
                <t t-set="div_01_class" t-value="'carousel-outer ks_vert_slide'"/>
                <t t-set="ol_class" t-value="'ks_vs_slide'"/>
                <t t-set="ol_div_class" t-value="'ks_vert_slider owl-carousel ks_thumb owl-theme'"/>
            </t>
        </t>
        <t t-else="">
            <t t-call="ks_theme_kinetik.ks_product_image_slider">
                <t t-set="div_01_class" t-value="'carousel-outer'"/>
                <t t-set="ol_class" t-value="'carousel-indicators'"/>
                <t t-set="ol_div_class" t-value="'ks_multi_image_horizontal owl-carousel ks_thumb owl-theme'"/>
            </t>
        </t>
    </template>

    <template id="ks_product_image_slider" name="Product image slider">
        <t t-set="variant_img" t-value="product_variant and product_variant.image_variant"/>
        <t t-set="image_ids" t-value="product.product_image_ids"/>
        <div id="o-carousel-product" class="ks_horizontal_product_slider carousel slide"
             data-ride="carousel" data-interval="0">
            <div t-attf-class="{{div_01_class}}">
                <div class="owl-carousel ks_main owl-theme">
                    <div data-oe-id="0" t-if="variant_img" class="ks_img_vrnt carousel-item active"
                         itemprop="image"
                         t-field="product_variant.image"
                         t-options="{'widget': 'image', 'class': 'product_detail_img js_variant_img', 'alt-field': 'name', 'zoom': 'image', 'unique': str(product['__last_update']) + (str(product_variant['__last_update']) or '')}"/>
                    <div t-att-data-oe-id="1 if variant_img else '0'" t-attf-class="carousel-item active"
                         itemprop="image" t-field="product.image"
                         t-options="{'widget': 'image', 'class': 'product_detail_img', 'alt-field': 'name', 'zoom': 'image', 'unique': product['__last_update']}"/>
                    <t t-if="len(image_ids)" t-foreach="image_ids" t-as="pimg">
                        <t t-set="attachment_id"
                           t-value="request.env['ir.attachment'].search([('res_field', '=', 'product_video'), ('res_id', '=', pimg.id)]).id"/>
                        <t t-if="attachment_id">
                            <div t-att-data-oe-id="pimg_index + (variant_img and 2 or 1)"
                                 class="carousel-item active">
                                <iframe t-att-id="pimg.id" class="embed-responsive-item"
                                        t-att-src="'/web/content/' + str(attachment_id)"
                                        allowFullScreen="true" frameborder="0" autoplay="1"
                                        allow="autoplay"/>
                            </div>
                        </t>
                        <t t-else="">
                            <div t-att-data-oe-id="pimg_index + (variant_img and 2 or 1)"
                                 class="carousel-item active" t-field="pimg.image"
                                 t-options='{"widget": "image", "class": "product_detail_img", "alt-field": "name", "zoom": "image" }'/>
                        </t>
                    </t>
                </div>
            </div>
            <ol t-attf-class="{{ol_class}}" t-if="len(image_ids) or variant_img">
                <div id="ks_multi_image_horiz" t-attf-class="{{ol_div_class}}">
                    <div t-if="variant_img" data-target="#o-carousel-product" data-slide-to="0"
                         class="ks_img_vrnt ks-vs-img ks_active_variant_image active">
                        <img class="img img-fluid js_variant_img_small"
                             t-attf-src="/web/image/product.product/{{product_variant.id}}/image/90x90"
                             t-att-alt="product.name"/>
                    </div>
                    <div data-target="#o-carousel-product"
                         t-att-data-slide-to="1 if variant_img else '0'"
                         t-att-class="'ks-vs-img ks_active_variant_image' if variant_img else 'ks-vs-img ks_active_variant_image active'">
                        <img class="img img-fluid"
                             t-attf-src="/web/image/product.template/{{product.id}}/image/90x90"
                             t-att-alt="product.name"/>
                    </div>
                    <t t-if="len(image_ids)" t-foreach="image_ids" t-as="pimg">
                        <div data-target="#o-carousel-product"
                             class="ks-vs-img ks_active_variant_image"
                             t-att-data-slide-to="pimg_index + (variant_img and 2 or 1)">
                            <t t-set="attachment_id"
                               t-value="request.env['ir.attachment'].search([('res_field', '=', 'product_video'), ('res_id', '=', pimg.id)]).id"/>
                            <img class="img img-fluid"
                                 t-attf-src="/website/image/product.image/{{pimg.id}}/image/90x90"
                                 t-att-alt="pimg.name"/>
                            <i t-if="attachment_id"
                               class="fa fa-2x fa-play-circle-o o_product_video_thumb"/>
                        </div>
                    </t>
                </div>
            </ol>
        </div>
    </template>
</odoo>