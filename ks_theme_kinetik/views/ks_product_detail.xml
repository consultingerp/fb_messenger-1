<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!--This is used to remove default template for rating and review option-->
    <template id="website_sale.product_comment" inherit_id="website_sale.product" customize_show="False"/>

    <!--This is used to remove default template for image zoom option-->
    <template inherit_id='website_sale.product' id="website_sale.product_picture_magnify" customize_show="False"/>

    <!--This is used to remove default template for auto image zoom option-->
    <template inherit_id='website_sale.product' id="website_sale.product_picture_magnify_auto" customize_show="False"/>
    <template id="ks_Breadcrumb_product" name="Kinetik Product Page Breadcrumb"
              inherit_id="website_sale.product" priority="100">
        <xpath expr="//div[@id='wrap']/section[1]" position="before">
            <ol class="breadcrumb">
                <li class="breadcrumb-item">
                    <a t-att-href="keep(category=0)">Products</a>
                </li>
                <li t-if="category" class="breadcrumb-item">
                    <a t-att-href="keep('/shop/category/%s' % slug(category), category=0)"
                       t-field="category.name"/>
                </li>
                <li class="breadcrumb-item active">
                    <span t-field="product.name"/>
                </li>
            </ol>
        </xpath>
    </template>

    <template id="ks_Breadcrumb_product" name="Kinetik Product Page Breadcrumb"
              inherit_id="website_sale.product" priority="100">
        <xpath expr="//div[@id='wrap']/section[1]" position="before">
             <div class="breadcrumb ks-bread-bg-color">
                <div class="container">
                    <div class="breadcrumb-item row align-items-center">
                        <div class="ml-auto">
                            <ol class="breadcrumb">
                                <li class="breadcrumb-item active"><a href="/"><i class="fa fa-home"></i></a> </li>
                                <li class="breadcrumb-item"><a href="/shop">Shop</a> </li>
<!--                                <li class="breadcrumb-item"><a t-att-href="keep(category=0)">Products</a></li>-->
                                <li t-if="category" class="breadcrumb-item"><a t-att-href="keep('/shop/category/%s' % slug(category), category=0)" t-field="category.name"/></li>
                                <li class="breadcrumb-item active"><span t-field="product.name"/></li>
                            </ol>
                        </div>
                    </div>
                </div>
            </div>
        </xpath>
    </template>

    <!--This is used to replace and make design of product detail page as our need-->
    <template id="kps_product" name="Kinetik Manager" inherit_id="website_sale.product">
        <xpath expr="//div[1]" position="replace">
            <div itemscope="itemscope" itemtype="http://schema.org/Product" id="wrap" class=" ks_temp js_sale">
                <section
                        t-attf-class="container py-2 oe_website_sale #{'discount' if combination_info['has_discounted_price'] else ''}"
                        id="product_detail">
                    <div class="row">
                        <div class="col-md-4">
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-6 product-detail-carousel">
                            <t t-call="ks_theme_kinetik.ks_product_image_slider">
                                <t t-set="div_01_class" t-value="'carousel-outer'"/>
                                <t t-set="ol_class" t-value="'carousel-indicators'"/>
                                <t t-set="ol_div_class"
                                   t-value="'ks_multi_image_horizontal owl-carousel ks_thumb owl-theme'"/>
                            </t>
                        </div>
                        <div class="col-md-6" id="product_details">
                            <div class="ks-breadcrumb-container" >
                                <ol class="breadcrumb p-0 d-flex justify-content-end">
                                    <li class="breadcrumb-item">
                                        <a t-att-href="keep(category=0)">Products</a>
                                    </li>
                                    <li t-if="category" class="breadcrumb-item">
                                        <a t-att-href="keep('/shop/category/%s' % slug(category), category=0)"
                                           t-field="category.name"/>
                                    </li>
                                    <li class="breadcrumb-item active">
                                        <span t-field="product.name"/>
                                    </li>
                                </ol>
                            </div>
                            <div class="ks_product_data_info flex-wrap">
                                 <div class="w-100 ks_product_timer">
                                     <div class="product_timer flex-wrap">
                <!--                         <t t-if="seconds">-->
                                             <div class="ks_timer_box mb-4 d-none">
                                                <div class="ks_product_timer_title text-center text-lg-left col-12  col-lg-4 col-xl-6">Offer Ends In !!</div>
                                                <input type="hidden" name="timer"
                                                   t-att-value="seconds"/>
                                                <!--<t t-if="seconds">-->
                                                <div class="clock col"></div>
                                                <!--</t>-->
                                             </div>
                <!--                         </t>-->
                                    </div>
                                </div>
                                <h1 itemprop="name" t-field="product.name" class="ks-product-name">Product Name</h1>
                            </div>
                            <t t-if="request.website.viewref('ks_theme_kinetik.ks_product_comment').active">
                                <t t-call="ks_theme_kinetik.ks_rating_widget_stars_static">
                                    <t t-set="rating_avg" t-value="product.rating_avg*2"/>
                                    <t t-set="rating_count" t-value="product.rating_count"/>
                                </t>
                            </t>
                            <t t-if="product.description_sale">
                                <div class="product_short_description">
                                    <t t-esc="product.description_sale"/>
                                </div>
                            </t>
                            <span itemprop="url" style="display:none;"
                                  t-esc="'%sshop/product/%s' % (request.httprequest.url_root, slug(product))"/>
                            <form t-if="product._is_add_to_cart_possible()" action="/shop/cart/update" method="POST">
                                <t t-call="website_sale.product_price"/>
                                <input type="hidden" name="csrf_token" t-att-value="request.csrf_token()"/>
                                <div class="js_product js_main_product">
                                    <t t-placeholder="select">
                                        <input type="hidden" class="product_id" name="product_id"
                                               t-att-value="product_variant.id"/>
                                        <input type="hidden" class="product_template_id" name="product_template_id"
                                               t-att-value="product.id"/>
                                        <t t-if="first_possible_combination" t-call="sale.variants">
                                            <t t-set="ul_class" t-value="'flex-column'"/>
                                            <t t-set="parent_combination" t-value="None"/>
                                        </t>
                                        <t t-else="">
                                            <ul class="d-none js_add_cart_variants"
                                                t-att-data-attribute_exclusions="{'exclusions: []'}"/>
                                        </t>
                                    </t>
                                    <div class="ks_action_buttons d-flex align-items-center">
                                        <a role="button" id="add_to_cart" class="btn btn-primary btn-lg js_check_product a-submit theme-btn cart__btn justify-content-center ks_modal_optional"
                                           href="#"
                                            t-att-data-product-product-id="product_variant.id"
                                                       t-att-data-product-template-id="product.id"
                                            >
                                            Add to Cart
                                        </a>
                                    </div>
                                </div>
                            </form>
                             <p t-if="True" id="ks_alert_wrng" class="css_not_available_msg alert alert-warning">This combination
                                        does not exist.
                                    </p>
                            <p t-elif="not product.active" class="alert alert-warning">This product is no longer
                                available.
                            </p>
                            <p t-else="" class="alert alert-warning">This product has no valid combination.</p>
                             <t t-if="ks_brand_icon or ks_brand_description"  >
                            <div class="row align-items-center no-gutters ks_brand_panel">
                                <t t-if="ks_brand_icon">
                                    <div class="col-4">
                                        <div class="ks_brand_thum_image">
                                            <img t-att-src="'/web/image/ks_product_manager.ks_brand/%s/ks_brand_logo/150x150' %(ks_brand_icon)"/>
                                        </div>
                                    </div>
                                </t>
                                 <t t-if="ks_brand_description">
                                     <div class="col">
                                         <div class="ks_brand_desc_content">
                                            <t t-esc="ks_brand_description"/>
                                         </div>
                                     </div>
                                </t>
                            </div>
                             </t>
                        </div>
                        <form t-if="product._is_add_to_cart_possible()" action="/shop/cart/update" method="POST">
                            <input type="hidden" name="csrf_token" t-att-value="request.csrf_token()"/>
                            <input type="hidden" class="product_id" name="product_id"
                                   t-att-value="product_variant.id"/>
                            <input type="hidden" class="product_template_id" name="product_template_id"
                                   t-att-value="product.id"/>
                            <div class="custom_add_to_cart_and_call" data-id="0">
                                <t t-if="request.website.viewref('ks_theme_kinetik.call_us').active">
                                    <a role="button" id="ks_call_id" class="ks_call_button"
                                       t-attf-href="tel:{{ res_company.phone }}">
                                        <i class="fa fa-phone"></i>
                                    </a>
                                </t>
                                <input type="text" class="mobile-add d-none" data-min="1" name="add_qty"
                                  value=""/>
                                <a role="button" id="add_to_cart"
                                   class="btn btn-primary btn-lg js_check_product  a-submit cart__btn justify-content-center ks_modal_optional add_cart_mobile"
                                   href="#"
                                   t-att-data-product-product-id="product_variant.id"
                                   t-att-data-product-template-id="product.id"
                                >
                                    Add To Cart
                                </a>

                            </div>
                        </form>
                        <div class="w-100 mb-3 mb-lg-5 ks-product-description">
                            <ul id="productTab" role="tablist" class="nav nav-tabs justify-content-center">
                                <li class="px-3 nav-item">
                                    <a id="description-tab" data-toggle="tab" href="#description" role="tab"
                                       aria-controls="description"
                                       aria-selected="true" class="nav-link px-2 active show">Description
                                    </a>
                                </li>
                                <li class="px-3 nav-item">
                                    <a id="details-tab" data-toggle="tab" href="#details" role="tab"
                                       aria-controls="details"
                                       aria-selected="false" class="nav-link px-2">Details
                                    </a>
                                </li>
                            </ul>
                            <div id="productTabContent" class="tab-content px-lg-4 py-4">
                                <div id="description" role="tabpanel" aria-labelledby="description-tab"
                                     class="tab-pane fade active show">
                                    <div itemprop="description" class="oe_structure mt16" t-field="product.website_description" id="product_full_description" />
                                    <br/>
                                </div>
                                <div id="details" role="tabpanel" aria-labelledby="details-tab" class="tab-pane fade">
                                    <table class="table details-table table-bordered">
                                           <t t-if="product.ks_product_brand_id.name">
                                                 <tr>
                                                    <th>Brand</th>
                                                     <td>
                                                         <t t-esc="product.ks_product_brand_id.name"/>
                                                     </td>
                                                </tr>
                                            </t>
                                           <t t-if="product.public_categ_ids">
                                                 <tr>
                                                    <th>Category</th>
                                                     <td>
                                                         <t t-set="i" t-value="0"/>
                                                         <t t-set="k" t-value="0"/>
                                                         <t t-foreach="product.public_categ_ids" t-as="categ">
                                                             <t t-set="i" t-value="i+1"/>
                                                         </t>
                                                        <t t-foreach="product.public_categ_ids" t-as="categ">
                                                            <t t-set="k" t-value="k+1"/>
                                                            <t t-if=" k != i">
                                                                <t t-esc="categ.name"/>
                                                                <span>,</span>
                                                            </t>
                                                            <t t-else="">
                                                                <t t-esc="categ.name"/>
                                                            </t>
                                                        </t>
                                                     </td>
                                                </tr>
                                           </t>
                                            <t t-if="product.weight">
                                                 <tr>
                                                    <th>Weight</th>
                                                     <td>
                                                        <t t-esc="product.weight"/>
                                                        <t t-esc="product.weight_uom_name"/>
                                                     </td>
                                                </tr>
                                            </t>
                                            <t t-if="product.volume">
                                                <tr>
                                                    <th>Volume</th>
                                                    <td>
                                                        <t t-esc="product.volume"/>
                                                        m³
                                                    </td>
                                                 </tr>
                                            </t>
                                        <t t-if="product.type">
                                            <tr>
                                                <th>Type</th>
                                                <td>
                                                    <t t-if="product.type == 'consu'">
                                                        <span>Consumable</span>
                                                    </t>
                                                    <t t-if="product.type == 'product'">
                                                        <span>Storable Product</span>
                                                    </t>
                                                    <t t-if="product.type == 'service'">
                                                        <span>Service</span>
                                                    </t>
                                                </td>
                                            </tr>
                                        </t>
                                    </table>
                                </div>
                                <div id="reviews" role="tabpanel" aria-labelledby="reviews-tab" class="tab-pane fade">
                                    <!--This div is used to place review and rating under review option of our owm-->
                                    <div class="o_shop_discussion_rating ks_rating">
                                        <section class="container mt16 mb16">
                                            <div class="row">
                                                <div class="col-lg-8 offset-lg-2">
                                                    <t t-call="portal.message_thread">
                                                        <t t-set="object" t-value="product"/>
                                                        <t t-set="display_rating" t-value="True"/>
                                                    </t>
                                                </div>
                                            </div>
                                        </section>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </section>
<!--                <div itemprop="description" t-field="product.website_description" />-->
            </div>
        </xpath>
    </template>
    <template id="ks_product_offer_detail_page" inherit_id="ks_theme_kinetik.kps_product" name="Product Size Chart" customize_show="True">
        <xpath expr="//div[@id='product_details']/div[2]/h1" position="after">
            <t t-if="product.size_chart">
                <span class="ks_product_pdf">
                    <input type="hidden" name="product_tmp_id" t-att-value="product.id"/>
                    <button class="btn btn-size-chart" data-toggle="modal" data-target="#size_chart_modal">
                        <span class="fa fa-eye mr-2 "/>
                        <span>Size Chart</span>
                    </button>
                </span>
            </t>
        </xpath>
        <xpath expr="//div[@id='product_details']" position="after">
            <!-- size chart Modal -->
            <div id="size_chart_modal" class="modal fade" role="dialog">
                <div class="modal-dialog size_chart_preview_modal">
                    <div class="modal-content">
                        <div class="modal-header">
                            <h4 class="modal-title">
                                <span>Product Size Chart</span>
                            </h4>
                            <button type="button" class="close" data-dismiss="modal">
                                <span class="fa fa-close"/>
                            </button>
                        </div>
                        <div class="modal-body">
                            <iframe t-att-src="'/product/sizechart/size_chart/'+str(product.id)" method="get"
                                    height="100%" width="100%"/>
                        </div>
                        <div class="modal-footer justify-content-start">
                            <a class="ks_dwnld"  t-att-href="'/product/sizechart/download/'+str(product.id)">
                                <button type="button" title="Download" aria-label="Download" class="btn btn-primary pull-right mr-8">
                                    <span class="fa fa-download"/>
                                </button>
                            </a>
                        </div>
                    </div>
                </div>
            </div>
        </xpath>
    </template>

    <template id="ks_product_attributes" inherit_id="ks_theme_kinetik.kps_product" name="Product attributes" >
        <xpath expr="//div[@id='details']" position="inside">
            <hr t-if="sum([(1 if len(l.value_ids)==1 else 0) for l in product._get_valid_product_template_attribute_lines()])" id='hr_product_attributes_simple'/>
            <p class="text-muted" id="product_attributes_simple"/>
            <table class="table details-table table-bordered">
                <t t-foreach="product._get_valid_product_template_attribute_lines()" t-as="variant_id">
                    <t t-if="len(variant_id.value_ids)==1">
                            <tr>
                               <th>
                                    <span t-field="variant_id.attribute_id" />:
                               </th>
                                <td>
                                    <span t-field="variant_id.value_ids[0].name" />
                                </td>
                            </tr>
                    </t>
                </t>
             </table>
        </xpath>
    </template>

    <template id = "website_sale.recommended_products" inherit_id="website_sale.product" customize_show="True" name="Alternative Products" active="False">
        <xpath expr="//section[@id='product_detail']" position="after">
             <div class="w-100 d-flex justify-content-center row ks-alternate-product-row">
                 <div class="ks_alternate_slider container mt32 col-md-6 col-12 pr-xl-5 mb-5" t-if="product.alternative_product_ids and product.ks_is_alternate_slider">
                        <h3 class="theme__heading">Alternative Products</h3>
                        <div class="row mt16 owl-carousel owl-theme alternate-prod-owl" style="">
                            <t t-foreach="product.alternative_product_ids" t-as="alt_product">
                                <div class="mt-2">
                                    <a t-attf-href="/shop/product/#{ slug(alt_product) }"
                                       style="display: block">
                                        <div class="mt16 text-center">
                                            <div t-field="alt_product.image"
                                                 t-options="{'widget': 'image', 'class': 'rounded shadow o_alternative_product' }"/>
                                            <div class="fp__name text-truncate text-left">
                                                <span t-att-title="alt_product.name" t-field="alt_product.name"
                                                      class="o_text_overflow" style="display: block;"/>
                                            </div>
                                            <div class="fp__price ks_price_info d-flex align-items-center">
                                                <div class="fp__current_price d-flex">
                                                    <span t-if="alt_product.price" t-esc="alt_product.price"
                                                          t-options="{'widget': 'monetary', 'display_currency': website.currency_id}"/>
                                                    <span itemprop="priceCurrency" style="display:none;"
                                                          t-esc="website.currency_id.name"/>
                                                </div>
                                                <t t-if="alt_product['list_price'] == 0" t-set="percentage_discounts"
                                                       t-value="0"/>
                                                <t t-else="" t-set="percentage_discounts"
                                                   t-value="((alt_product['list_price']-alt_product['price'])/alt_product['list_price'])*100"/>
                                                <t t-if="percentage_discounts > 0">
                                                    <del t-attf-class="ks_public_price"
                                                         style="white-space: nowrap;"
                                                         t-esc="alt_product['list_price']"
                                                         t-options="{'widget': 'monetary', 'display_currency': website.currency_id}"/>
                                                </t>
                                                <t t-if="request.website.viewref('ks_theme_kinetik.percent_discount').active">
                                                    <t t-if="percentage_discounts > 0">
                                                        <div class="ks_snippet_percentage_discount_2">
                                                            <div class="ks_Percentage-offer">
                                                                <span>(</span>
                                                                <t t-esc="int(percentage_discounts)"/>%
                                                                <span class="ks_percentage_info">off)</span>
                                                            </div>
                                                        </div>
                                                    </t>
                                                </t>
                                            </div>
                                        </div>
                                    </a>
                                </div>
                            </t>
                        </div>
                    </div>
                 <div class="ks_alternate_slider ks_accessories_slider container mt32 col-md-6 col-12 pl-xl-5 mb-5" t-if="product.accessory_product_ids and product.ks_is_accessories_slider">
                        <h3 class="theme__heading">Accessories</h3>
                        <div class="row mt16 owl-carousel owl-theme accessories-prod-owl" style="">
                            <t t-foreach="product.accessory_product_ids" t-as="alt_product">
                                <div class="mt-2" >
                                    <a t-attf-href="/shop/product/#{ slug(alt_product.product_tmpl_id) }"
                                       style="display: block">
                                        <div class="mt16 text-center">
                                            <div t-field="alt_product.image"
                                                 t-options="{'widget': 'image', 'class': 'rounded shadow o_accessory_product' }"/>
                                            <div class="fp__name text-truncate text-left">
                                                <span t-att-title="alt_product.name" t-field="alt_product.name"
                                                      class="o_text_overflow" style="display: block;"/>
                                            </div>
                                            <div class="fp__price ks_price_info d-flex align-items-center">
                                                <div class="fp__current_price d-flex">
                                                    <span t-if="alt_product.price" t-esc="alt_product.price"
                                                          t-options="{'widget': 'monetary', 'display_currency': website.currency_id}"/>
                                                    <span itemprop="priceCurrency" style="display:none;"
                                                          t-esc="website.currency_id.name"/>
                                                </div>
                                                <t t-if="alt_product['list_price'] == 0" t-set="percentage_discounts"
                                                       t-value="0"/>
                                                <t t-else="" t-set="percentage_discounts"
                                                   t-value="((alt_product['list_price']-alt_product['price'])/alt_product['list_price'])*100"/>
                                                <t t-if="percentage_discounts > 0">
                                                    <del t-attf-class="ks_public_price"
                                                         style="white-space: nowrap;"
                                                         t-esc="alt_product['list_price']"
                                                         t-options="{'widget': 'monetary', 'display_currency': website.currency_id}"/>
                                                </t>
                                                <t t-if="request.website.viewref('ks_theme_kinetik.percent_discount').active">
                                                    <t t-if="percentage_discounts > 0">
                                                        <div class="ks_snippet_percentage_discount_2">
                                                            <div class="ks_Percentage-offer">
                                                                <span>(</span>
                                                                <t t-esc="int(percentage_discounts)"/>%
                                                                <span class="ks_percentage_info">off)</span>
                                                            </div>
                                                        </div>
                                                    </t>
                                                </t>

                                            </div>
                                        </div>
                                    </a>
                                </div>
                            </t>
                        </div>
                    </div>
             </div>
        </xpath>
    </template>

    <template id="ks_multiimage_layout" inherit_id='ks_theme_kinetik.kps_product' active="False"
              customize_show="True" name="Kinetik Multi Image Vertically">
        <xpath expr="//div[hasclass('product-detail-carousel')]" position="replace">
            <div class="col-md-6 product-detail-carousel">
                <t t-call="ks_theme_kinetik.ks_product_image_slider">
                    <t t-set="div_01_class" t-value="'carousel-outer ks_vert_slide'"/>
                    <t t-set="ol_class" t-value="'ks_vs_slide'"/>
                    <t t-set="ol_div_class" t-value="'ks_vert_slider owl-carousel ks_thumb owl-theme'"/>
                </t>
            </div>
        </xpath>
    </template>

    <!--In this template we have inherited own template to show rating if user want and make it active from customize-->
    <template id="ks_product_comment" inherit_id="ks_theme_kinetik.kps_product" active="False"
              customize_show="True"
              name=" Kinetik Rating and review">
       <xpath expr="//div[hasclass('ks-product-description')]//ul[@id='productTab']" position="inside">
            <li class="nav-item">
                <a id="reviews-tab" data-toggle="tab" href="#reviews" role="tab"
                   aria-controls="reviews"
                   aria-selected="false" class="nav-link px-2">reviews
                </a>
            </li>
        </xpath>
    </template>

    <!--This is used to make all image zoomable on click -->
    <template inherit_id='ks_theme_kinetik.kps_product' id="product_picture_magnify" customize_show="True"
              name="Kinetik Image Zoom">
        <xpath expr='//div[hasclass("js_sale")]' position='attributes'>
            <attribute name="class" separator=" " add="ecom-zoomable zoomodoo-next"/>
        </xpath>
    </template>

    <!--This is used to make all image zoomable automitically on hover -->
    <template inherit_id='ks_theme_kinetik.kps_product' id="product_picture_magnify_auto" active="False"
              customize_show="True" name=" Kinetik Automatic Image Zoom">
        <xpath expr='//div[hasclass("js_sale")]' position='attributes'>
            <attribute name="data-ecom-zoom-auto">1</attribute>
            <attribute name="class" separator=" " add="ecom-zoomable zoomodoo-next"/>
        </xpath>
    </template>

